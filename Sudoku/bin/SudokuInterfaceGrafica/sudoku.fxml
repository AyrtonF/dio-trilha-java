<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SudokuInterfaceGrafica.SudokuController">
    <GridPane fx:id="tabuleiroGrid" hgap="5" vgap="5">
        <padding>
            <Insets top="20" right="20" bottom="20" left="20"/>
        </padding>
        <!-- Células do Sudoku serão adicionadas dinamicamente -->
    </GridPane>
    <Button text="Verificar" onAction="#verificarTabuleiro" />
</VBox>